    <title>Sign Up</title>
  </head>
  <body>
  
<header>
      <nav class="nav collapsible">
        <div class="xx_nav_brand">
          <img src="/static/assets/rocket.svg" id="xx_brand_logo"> 
          <span><a href="../" class="normal-link">Rocket Share</a></span>
        </div>
